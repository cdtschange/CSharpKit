        <my:Card Height="96" HorizontalAlignment="Left" Margin="309,183,0,0" Name="card7" VerticalAlignment="Top" Width="71" />
        <my:Card Height="96" HorizontalAlignment="Left" Margin="430,0,0,96" Name="card8" VerticalAlignment="Bottom" Width="71" />
