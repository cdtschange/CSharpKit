                    <Storyboard>
                        <DoubleAnimation Duration="0:0:0.2" To="-10" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Storyboard.TargetName="paperChooseImg" d:IsOptimized="True"/>
                    </Storyboard>
                </VisualState>
                <VisualState x:Name="defaultStory"/>
